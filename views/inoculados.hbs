        </button>
                    </div>
                </div>

                <table class="table table-striped table-sm table-success">
                                <thead>
                                    <tr>
                                        <th scope="col" class="text-center">Cédula</th>
                                        <th scope="col" class="text-center">Nombre</th>
                                        <th scope="col" class="text-center">Edad</th>
                                        <th scope="col" class="text-center">Vacuna</th>
                                        
                                        <th scope="col" class="text-center">Razón</th>
                                        <th scope="col" class="text-center">Dosis</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    
                                        {{#if ciudadanos}}
                                        
                                        {{#each ciudadanos}}
                                        <tr>
                                        <td class="text-center">{{this.cedula}}</td>
                                        <td class="text-center">{{this.nombre}}</td>
                                        <td class="text-center">{{this.nombre}}</td>
                                        <td class="text-center">{{this.nombre}}</td>
                                        <td class="text-center">{{this.nombre}}</td>
                                        <td class="text-center">{{this.nombre}}</td>
                                        </tr>

                                        {{/each}}
                                        
                                        {{else}}
                                        <tr>
                                        <td class="text-center">Vacío</td>
                                        <td class="text-center">Vacío</td>
                                        <td class="text-center">Vacío</td>
                                        <td class="text-center">Vacío</td>
                                        <td class="text-center">Vacío</td>
                                        <td class="text-center">Vacío</td>
                                        </tr>
                                        {{/if}}

                                </tbody>

